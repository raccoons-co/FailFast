{
  "name": "failfast",
  "version": "0.0.1",
  "description": "FailFast is the TypeScript annotation to support Test-driven development discipline.",
  "files": [
    "lib/"
  ],
  "main": "lib/main/index",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsc",
    "lint.packagejson": "npmPkgJsonLint .",
    "lint.typescript": "eslint './src/**/*.ts'",
    "posttest": "npm-run-all lint.*",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "test": "node lib/test/EntryPoint"
  },
  "keywords": [
    "TypeScript",
    "TDD"
  ],
  "author": "Raccoons <info@raccoons.co>",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.34.0",
    "npm-package-json-lint": "^6.4.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=16.x"
  }
}
